                                },
                                "user_id": {
                                    "type": "string"
                                },
                                "permissions": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "iat": {
                                    "type": "integer"
                                },
                                "lua": {
                                    "type": "integer"
                                },
